from tkinter import *
import customtkinter as ctk
import pygame
window = ctk.CTk()
window.title('MP3 Player')
window.iconbitmap("mp3.png")
window.geometry("500x400")

pygame.mixer.init()
song_Box = Listbox(window, bg="gray", fg="orange", width=60)
song_Box.pack(pady=20)

def Play():
    pygame.mixer.music.load("")
my_button = Button(window, text="Play this song", font=("Helvitica", 32), command=Play)
my_button.pack(pady=20)
window.mainloop()
